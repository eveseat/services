{
  "name": "eveseat/services",
  "description": "SeAT Services Components",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Leon Jacobs",
      "email": "leonja511@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seat\\Services\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "ext-dom": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "composer-runtime-api": "^2.0",
    "coduo/php-humanizer": "^4.0",
    "composer/semver": "^3.0",
    "doctrine/dbal": "^3.0",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "^8.0",
    "web-token/jwt-easy": "^2.1",
    "web-token/jwt-signature-algorithm-hmac": "^2.1",
    "web-token/jwt-signature-algorithm-rsa": "^2.1",
    "web-token/jwt-signature-algorithm-ecdsa": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seat\\Services\\ServicesServiceProvider"
      ]
    }
  }
}
