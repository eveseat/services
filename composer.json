{
  "name": "eveseat/services",
  "description": "SeAT Services Components",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Leon Jacobs",
      "email": "leonja511@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seat\\Services\\": "src/",
      "Seat\\Services\\Database\\Seeders\\": "src/database/seeders/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Seat\\Tests\\Services\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-dom": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "composer-runtime-api": "^2.0",
    "coduo/php-humanizer": "^4.0",
    "composer/semver": "^3.0",
    "doctrine/dbal": "^3.0",
    "eveseat/eseye": "^3.x-dev",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "^10.0",
    "psr/simple-cache": "^3.0",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seat\\Services\\ServicesServiceProvider"
      ]
    }
  }
}
